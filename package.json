{
  "name": "peter",
  "version": "3.0.0",
  "description": "A feature rich Discord music bot that offers features such as 24/7 and audio effects for free.",
  "main": "./dist/index.js",
  "scripts": {
    "build": "rm -rf ./dist && tsc",
    "build:install": "npm i --also=dev && npm run build",
    "lint": "eslint ./src",
    "lint:fix": "eslint ./src --fix",
    "lint:install": "npm i --also=dev && npm run lint",
    "start": "node ./dist/index.js",
    "test:build": "npm run build:install",
    "test:lint": "npm run lint:install"
  },
  "keywords": [
    "bot",
    "discord",
    "discord-api",
    "discord-bot",
    "music",
    "peter"
  ],
  "homepage": "https://guidetothe.net",
  "bugs": {
    "url": "https://github.com/BR88C/peter/issues"
  },
  "license": "GPL-3.0",
  "author": "BR88C",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BR88C/peter.git"
  },
  "dependencies": {
    "@br88c/node-utils": "^1.1.3",
    "@distype/cmd": "^0.1.0",
    "distype": "^0.10.1",
    "dotenv": "^16.0.0"
  },
  "devDependencies": {
    "@br88c/eslint-config": "^1.4.0",
    "@types/node": "^16.11.27",
    "eslint": "^8.13.0",
    "typescript": "^4.6.3"
  },
  "engines": {
    "node": ">=16.13.0",
    "npm": ">=8.1.0"
  },
