###########################

# The bot's token.
BOT_TOKEN="your_bot_token"

# The InfluxDB bucket to use.
INFLUX_BUCKET="example"

# The InfluxDB organization to use.
INFLUX_ORG="example"

# The InfluxDB token to use.
INFLUX_TOKEN="example"

# The InfluxDB URL to use.
INFLUX_URL="https://influx.example.com"

# The Lavalink server's host.
LAVALINK_HOST="localhost"

# The Lavalink server's port.
LAVALINK_PORT="2333"

# If the Lavalink server's connection is secure.
# Should be "true" or "false".
LAVALINK_SECURE="false"

# The lavalink server's password.
LAVALINK_PASSWORD="youshallnotpass"


###########################
